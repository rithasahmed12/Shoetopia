<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Blocked User</title>
    <!-- Include SweetAlert CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/sweetalert2@10">
</head>
<body>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>
    
<script>
    // Use SweetAlert to display a message
    Swal.fire({
        title: 'Blocked User',
        text: 'Your account has been blocked. Please contact support@example.com for assistance.',
        icon: 'warning',
        confirmButtonText: 'OK'
    }).then(() => {
        // Redirect the user to the homepage or another relevant page
        window.location.href = '/';
    });
</script>

<!-- Include SweetAlert JS -->

</body>
</html>
